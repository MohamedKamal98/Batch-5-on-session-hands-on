<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <test  name="Test 1 Parametrization">
        <classes>
            <class name="data_driven.ParameterizingTest">
                <parameter name="username" value="Mohamed"></parameter>
                <parameter name="password" value="Mohamed Password"></parameter>
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>
    <test  name="Test 2 Parametrization">
        <classes>
            <class name="data_driven.ParameterizingTest">
                <parameter name="username" value="Kamal"></parameter>
                <parameter name="password" value="Kamal Password"></parameter>
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
